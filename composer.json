{
    "name": "kleitomberg/phpdcsd",
    "description": "Detecção Code Smells Symfony Doctrine",
    "autoload": {
        "psr-4": {
            "Phpdcsd\\": "src/",
            "Phpdcsd\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Kleitomberg",
            "email": "51670381+Kleitomberg@users.noreply.github.com"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    },
    "scripts": {
        "run-test": "vendor/bin/phpstan analyse",
        "run-test-verbose": "vendor/bin/phpstan analyse src -v",
        "run-debug": "vendor/bin/phpstan analyse --debug",
        "clear-cache": "vendor/bin/phpstan clear-result-cache"
    },
    "require": {
        "nikic/php-parser": "^4.17",
        "doctrine/annotations": "^2.0"
    }
}
